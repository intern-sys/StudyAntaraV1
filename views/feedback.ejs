<% include ./partials/header %>
<div class="card">
<div class="card-body">
  <h4 class="card-title text-center">Feedback</h4>
  <h6 class="card-subtitle text-muted text-center">Let us know what you think</h6>
  <form class="card-text w-75 mx-auto">
    <div class="form-group">
      <label for="fullName">Full Name</label>
      <input type="text" class="form-control" placeholder='Full Name'>
    </div>
    <div class="form-group">
      <label for="email">Email</label>
      <input type="email" class="form-control" placeholder='Email'>
    </div>

    <div class="form-group">
      <label for="Subject">Subject</label>
      <select class="form-control">
        <option>Question</option>
        <option>Courses</option>
        <option>Technical Issues</option>
        <option>Anything Else</option>
      </select>
    </div>
    <div class="form-group">
      <label for="FeedbackTextArea">Messege</label>
      <textarea class="form-control" rows="3"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Send</button>
  </form>

</div>
</div>
<% include ./partials/footer %>
